{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/test",
        "dest": "/api/test.js"
      },
      {
        "src": "/api/hello",
        "dest": "/api/hello.js"
      },
      {
        "src": "/api/articles",
        "dest": "/api/articles.js"
      },
      {
        "src": "/api/static-articles",
        "dest": "/api/static-articles.js"
      },
      {
        "src": "/api/static-article-content",
        "dest": "/api/static-article-content.js"
      },
      {
        "src": "/api/article-content/(.*)",
        "dest": "/api/article-content/[id].js?id=$1"
      },
      {
        "src": "/scripts/config/config\\.development\\.js",
        "dest": "/scripts/config/config.development.js",
        "headers": {
          "cache-control": "no-cache, no-store, must-revalidate"
        }
      },
      {
        "src": "/scripts/core/(.*)\\.js",
        "dest": "/scripts/core/$1.js"
      },
      {
        "src": "/styles/(.*)",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        },
        "dest": "/styles/$1"
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)\\.html",
        "dest": "/$1.html"
      },
      {
        "src": "/(.*)",
        "dest": "/$1",
        "headers": {
          "x-content-type-options": "nosniff",
          "x-frame-options": "DENY",
          "x-xss-protection": "1; mode=block",
          "referrer-policy": "strict-origin-when-cross-origin"
        }
      }
    ]
  }