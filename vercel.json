{
    "version": 2,
    "builds": [
      {
        "src": "**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/scripts/(.*)",
        "dest": "/scripts/$1"
      },
      {
        "src": "/styles/(.*)",
        "dest": "/styles/$1"
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }